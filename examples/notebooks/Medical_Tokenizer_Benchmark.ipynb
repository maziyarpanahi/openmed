   "source": [
    "# Medical Tokenizer Benchmark\n",
    "Measure latency and entity stability with medical tokenizer on/off."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import time\n",
    "from statistics import mean\n",
    "from openmed import analyze_text\n",
    "from openmed.core import OpenMedConfig\n",
    "\n",
    "text = \"\"" + " ".join([
    "t(8;21) AML on daunorubicin/cytarabine 90mg/m2/day; WBC 12.3 x10^3/ÂµL, HbA1c 7.5%.",
    "IL-6-mediated cytokine storm post-CAR-T; tocilizumab 8mg/kg and methylpred 1 mg/kg.",
    "COVID-19+ pt with O2 sat 88%, on 40% FiO2 via HFNC; dexamethasone 6 mg qd started."
    ]) + "\"\""\n",
    "\n",
    "def run(cfg, n=3):\n",
    "    times = []\n",
    "    for _ in range(n):\n",
    "        t0 = time.time()\n",
    "        res = analyze_text(text, model_name='oncology_detection_superclinical', config=cfg)\n",
    "        times.append(time.time() - t0)\n",
    "    return mean(times), res\n",
    "\n",
    "cfg_on = OpenMedConfig(use_medical_tokenizer=True)\n",
    "cfg_off = OpenMedConfig(use_medical_tokenizer=False)\n",
    "\n",
    "t_on, res_on = run(cfg_on)\n",
    "t_off, res_off = run(cfg_off)\n",
    "\n",
    "def summarize(res):\n",
    "    return [(e.label, e.text) for e in res.entities]\n",
    "\n",
    "print('Mean latency (s) with medical tokenizer:', round(t_on, 3))\n",
    "print('Mean latency (s) without:', round(t_off, 3))\n",
    "\n",
    "print('\nEntities with medical tokenizer (first 10):', summarize(res_on)[:10])\n",
    "print('\nEntities without medical tokenizer (first 10):', summarize(res_off)[:10])\n"
   ]
  }
